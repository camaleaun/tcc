prog algoritmo186
int a;
real numero;
para( a <- 1; a <=4; a++)
{
   imprima "\ndigite numero com ponto: ";
   leia numero;
   imprima  "\ncubo: ", numero ** 3;
   se(numero < 0.)
   { imprima "\nNao faco raiz de numero negativo: "; }
   senao
   { imprima "\nraiz: ", formatar(numero **(1/3),4); }
 }
imprima "\n";
fimprog
