prog algoritmo312
int num, cont, tot1, tot2, menor;
cont  <-  0;
tot1  <-  0;
tot2  <-  0;
imprima "\nDigite numero ou -1 para acabar: ";
leia num;
enquanto(num <> -1)
{
   tot1  <-  tot1 + num;
   cont ++;
   se(num % 8 == 0)
   {  tot2++; }
  imprima "\n", num;
  imprima "\nDigite numero ou -1 para acabar: ";
   leia num;
}
imprima "\nTotal de multiplos de 8: ", tot2;
imprima "\nMedia dos numeros: ", tot1/cont;
imprima "\n";
fimprog
