//prog algoritmo251
variaveis
numerico  L, c,linha 
caracter resp 
inicio
limpar
para  L = 1  ate  10
  linha=1
  para c=1  ate 10 
    se L <> c entao
      posicionar linha,1
      escrever "time : ",L
      posicionar linha,15
      escrever "x "
      posicionar linha,25
      escrever  "time : ", c, "     ________"   
      linha=linha+1
    fim_se
   proximo
   posicionar 24,1
   escrever "pressione enter:"
   posicionar 24,17
   ler resp
   limpar
proximo
fim
