//prog algoritmo2151
variaveis
numerico  c, num, linha, col
caracter resp
inicio
limpar
escrever "digite numero: " 
posicionar 1,16
ler num 
linha=3
col=1
para c = 1  ate  num
   se resto( num , c) = 0 entao
    posicionar linha,col  
    escrever c
   col=col+6
  fim_se
  se  col =73 entao
    linha =linha +1
    col=1
   fim_se
proximo
fim
