//prog algoritmo117solucao1
variaveis
numerico a, b, c,maior 
inicio
limpar 
escrever "digite 1 numero: "
posicionar 1,18  
ler a
escrever "digite 2 numero: " 
posicionar 2,18    
ler b
escrever "digite 3 numero: " 
posicionar 3,18      
ler c 
se a > b entao
  maior = a  
senao
maior = b   
fim_se
se c > maior entao
   maior = c 
fim_se
escrever "maior: ", maior 
fim
