//prog algoritmo278
variaveis
numerico num,cont,num1
inicio
limpar
escrever "Entre com o numero: "
ler num
cont  =  0
num1  =  2
faca enquanto (cont = 0 ) e ( num1<= inteiro(num / 2) )
    se resto( num , num1)=0 entao
     cont = 1  
    fim_se
    num1=num1+1
fim_enquanto
 se  cont = 1 entao
  escrever "nao e primo "
 senao
  escrever "e primo"
fim_se
fim
