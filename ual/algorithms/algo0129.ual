prog algoritmo129
real salario, desconto;
imprima "\ndigite salario: ";
leia salario;
se ( salario <= 600. )
{  desconto <- 0.;}
senao
{ se  ( salario <= 1200. )
 {  desconto <- salario * 0.2; }
 senao
 { se  ( salario <= 2000. )
   {  desconto <- salario * 0.25; }
   senao
   {  desconto <- salario * 0.30;}
 }
}
imprima "\ndesconto: ",desconto;
imprima "\n";
fimprog
