prog algoritmo278
int num,cont,num1;
imprima "\nEntre com o numero: ";
leia num;
cont  <-  0;
num1  <-  2;
enquanto(cont == 0 && num1<= num div  2)
 {
    se( num % num1==0 )
    { cont <- 1;  }
    num1++;
 }
 se( cont == 1 )
 { imprima "\nnao e primo "; }
 senao
 { imprima "\ne primo" ;}
imprima"\n";
fimprog
